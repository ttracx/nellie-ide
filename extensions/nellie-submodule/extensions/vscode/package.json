        "group": "Nellie IDE"
      },
      {
        "command": "nellie.fixCode",
        "category": "Nellie IDE",
        "title": "Fix this Code",
        "group": "Nellie IDE"
      },
      {
        "command": "nellie.optimizeCode",
        "category": "Nellie IDE",
        "title": "Optimize this Code",
        "group": "Nellie IDE"
      },
      {
        "command": "nellie.fixGrammar",
        "category": "Nellie IDE",
