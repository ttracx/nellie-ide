        "group": "Nellie IDE"
      },
      {
        "command": "nellie.codebaseForceReIndex",
        "category": "Nellie IDE",
        "title": "Codebase Force Re-Index",
        "group": "Nellie IDE"
      },
      {
        "command": "nellie.docsIndex",
        "category": "Nellie IDE",
        "title": "Docs Index",
        "group": "Nellie IDE"
      },
      {
        "command": "nellie.docsReIndex",
        "category": "Nellie IDE",
        "title": "Docs Force Re-Index",
        "group": "Nellie IDE"
      },
      {
        "command": "nellie.login",
        "category": "Nellie IDE",
        "title": "Login to Nellie IDE",
        "group": "Nellie IDE"
      },
      {
        "command": "nellie.logout",
        "category": "Nellie IDE",
        "title": "Logout of Nellie IDE",
        "group": "Nellie IDE"
      },
      {
        "command": "nellie.manualLogin",
        "category": "Nellie IDE Developer",
        "title": "Manual Nellie IDE Login",
        "group": "Nellie IDE"
      },
      {
        "command": "nellie.loadRecentChat",
        "category": "Nellie IDE",
        "title": "Load Recent Chat",
        "group": "Nellie IDE"
      },
      {
        "command": "nellie.resizeAuxiliaryBarWidth",
        "category": "Nellie IDE",
        "title": "Resize Chat",
        "group": "Nellie IDE"
      },
      {
        "command": "nellie.macResizeAuxiliaryBarWidth",
        "category": "Nellie IDE",
        "title": "Big Chat - CMD + [",
