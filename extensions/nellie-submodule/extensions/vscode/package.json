        "group": "Nellie IDE"
      },
      {
        "command": "nellie.focusAgentView",
        "category": "Nellie IDE",
        "title": "Focus Nellie IDE Agent View",
        "group": "Nellie IDE"
      },
      {
        "command": "nellie.focusNellie IDESearchView",
        "category": "Nellie IDE",
        "title": "Focus Nellie IDE Search View",
        "group": "Nellie IDE"
      },
      {
        "command": "nellie.focusNellie IDEMem0View",
