        "group": "Nellie IDE"
      },
      {
        "command": "nellie.debugTerminal",
        "category": "Nellie IDE",
        "title": "Nellie IDE: Debug Terminal",
        "group": "Nellie IDE"
      },
      {
        "command": "nellie.toggleFullScreen",
        "category": "Nellie IDE",
        "title": "Toggle Full Screen",
        "icon": "$(fullscreen)",
        "group": "Nellie IDE"
      },
      {
        "command": "nellie.openConfigJson",
        "category": "Nellie IDE",
        "title": "Open config.json",
        "group": "Nellie IDE"
      },
      {
        "command": "nellie.openPearAiWelcome",
        "category": "Nellie IDE",
        "title": "Open Welcome page for Nellie IDE",
        "group": "Nellie IDE"
      },
      {
        "command": "nellie.toggleTabAutocompleteEnabled",
        "category": "Nellie IDE",
        "title": "Toggle Autocomplete Enabled",
        "group": "Nellie IDE"
      },
      {
        "command": "nellie.selectFilesAsContext",
