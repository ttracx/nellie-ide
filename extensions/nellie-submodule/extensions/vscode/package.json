{
  "_comment": "This file has been modified significantly from its original contents. New commands have been added, and there has been renaming from Continue to Nellie IDE. nellie-submodule is a fork of Continue (https://github.com/continuedev/continue).",
  "name": "nellie-ide",
  "publisher": "nellieide",
  "icon": "media/icon.png",
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/nellie-ai/nellie-ide-submodule"
  },
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "bugs": {
    "url": "https://github.com/nellie-ai/nellie-ide-app/issues"
  },
  "exports": {
    ".": {
      "types": "./out/index.d.ts",
      "default": "./out/extension.js"
    }
  },
  "homepage": "https://nellie.ai",
  "qna": "https://github.com/nellie-ai/nellie-ide-app/issues/new/choose",
  "displayName": "Nellie IDE",
  "license": "Apache-2.0",
  "pricing": "Free",
  "description": "Supercharge your development with AI",
  "engines": {
    "vscode": "^1.70.0",
    "node": ">=20.18.0"
  },
  "engine-strict": true,
  "galleryBanner": {
    "color": "#1E1E1E",
    "theme": "dark"
  },
  "categories": [
    "AI",
    "Chat",
    "Programming Languages",
    "Education",
    "Machine Learning",
    "Snippets"
  ],
  "keywords": [
    "chatgpt",
    "github",
    "copilot",
    "claude",
    "sonnet",
    "mistral",
    "codestral",
    "codegpt",
    "ai",
    "llama"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "browser": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "filenames": [
          "config.json",
          ".nellierc.json"
        ],
        "id": "jsonc"
      }
    ],
    "configuration": {
      "title": "Nellie IDE",
      "properties": {
        "nellie.telemetryEnabled": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "Nellie IDE collects anonymous usage data, cleaned of PII, to help us improve the product for our users. Read more  at [nellie.dev › Telemetry](https://nellie.ai/telemetry)."
        },
        "nellie.enableContinueForTeams": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "_(Requires window reload)_ Enable Nellie IDE for teams beta features. To sign in, click the person icon in the bottom right of the sidebar."
        },
        "nellie.showInlineTip": {
          "type": "boolean",
          "default": true,
          "description": "Show inline suggestion to use the Nellie IDE keyboard shortcuts (e.g. \"Cmd/Ctrl L to select code, Cmd/Ctrl I to edit\")."
        },
        "nellie.enableQuickActions": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "Enable the experimental Quick Actions feature. Read our walkthrough to learn about configuration and how to share feedback: [nellie.dev › Walkthrough: Quick Actions (experimental)](https://nellie.ai/features/quick-actions)"
        },
        "nellie.enableTabAutocomplete": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "Enable Nellie IDE's tab autocomplete feature. Read our walkthrough to learn about configuration and how to share feedback: [nellie.dev › Walkthrough: Tab Autocomplete (beta)](https://nellie.ai/docs/tab-autocomplete)"
        },
        "nellie.pauseTabAutocompleteOnBattery": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "Pause Nellie IDE's tab autocomplete feature when your battery is low."
        },
        "nellie.pauseCodebaseIndexOnStart": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "Pause Nellie IDE's codebase index on start."
        },
        "nellie.enableDebugLogs": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "Enable Nellie IDE Debug Logs in the Output panel."
        },
        "nellie.remoteConfigServerUrl": {
          "type": "string",
          "default": null,
          "markdownDescription": "If your team is set up to use shared configuration, enter the server URL here and your user token below to enable automatic syncing."
        },
        "nellie.userToken": {
          "type": "string",
          "default": null,
          "markdownDescription": "If your team is set up to use shared configuration, enter your user token here and your server URL above to enable automatic syncing."
        },
        "nellie.remoteConfigSyncPeriod": {
          "type": "number",
          "default": 60,
          "description": "The period of time in minutes between automatic syncs."
        }
      }
    },
    "commands": [
      {
        "command": "nellie.acceptDiff",
        "category": "Nellie IDE",
        "title": "Accept Diff",
        "group": "Nellie IDE"
      },
      {
        "command": "nellie.rejectDiff",
        "category": "Nellie IDE",
        "title": "Reject Diff",
        "group": "Nellie IDE",
        "icon": "$(stop)"
      },
      {
        "command": "nellie.acceptVerticalDiffBlock",
        "category": "Nellie IDE",
        "title": "Accept Vertical Diff Block",
        "group": "Nellie IDE"
      },
      {
        "command": "nellie.rejectVerticalDiffBlock",
        "category": "Nellie IDE",
        "title": "Reject Vertical Diff Block",
        "group": "Nellie IDE"
      },
      {
        "command": "nellie.quickEdit",
        "category": "Nellie IDE",
        "title": "Generate Code",
        "group": "Nellie IDE"
      },
      {
        "command": "nellie.toggleCreator",
        "category": "Nellie IDE",
        "title": "Toggle Nellie IDE Creator",
        "group": "Nellie IDE"
      },
      {
        "command": "nellie.toggleSearch",
