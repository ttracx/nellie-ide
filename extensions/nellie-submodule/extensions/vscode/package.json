      },
      {
        "command": "nellie.winResizeAuxiliaryBarWidth",
        "category": "Nellie IDE",
        "title": "Big Chat - CTRL + [",
        "group": "Nellie IDE"
      },
      {
        "command": "nellie.patchWSL",
        "category": "Shell",
        "title": "Patch WSL",
        "group": "Shell"
      },
      {
        "command": "nellie.perplexityMode",
        "category": "Nellie IDE",
        "title": "Open Nellie IDE Search",
        "group": "Nellie IDE"
      },
      {
        "command": "nellie.showInteractiveContinueTutorial",
        "category": "Nellie IDE",
        "title": "Show Chat Tutorial",
        "group": "Nellie IDE"
      },
      {
        "command": "nellie.notifyOverlayOpened",
        "title": "Dev - Overlay Opened",
        "group": "Nellie IDE"
      }
    ],
    "keybindings": [
      {
        "command": "nellie.toggleOverlay",
        "mac": "cmd+e",
        "key": "ctrl+e"
      },
      {
        "command": "nellie.toggleInventorySettings",
        "mac": "cmd+shift+1",
        "key": "ctrl+shift+1"
      },
      {
        "command": "nellie.toggleCreator",
        "mac": "cmd+1",
        "key": "ctrl+1"
      },
      {
        "command": "nellie.toggleSearch",
        "mac": "cmd+2",
        "key": "ctrl+2"
      },
      {
        "command": "nellie.toggleMem0",
        "mac": "cmd+3",
        "key": "ctrl+4"
      },
      {
        "command": "nellie.focusContinueInputWithoutClear",
        "mac": "cmd+shift+l",
        "key": "ctrl+shift+l"
      },
      {
        "command": "nellie.toggleAuxiliaryBar",
        "mac": "alt+cmd+l",
        "key": "alt+ctrl+l"
      },
      {
        "command": "nellie.acceptDiff",
        "mac": "shift+cmd+enter",
        "key": "shift+ctrl+enter"
      },
      {
        "command": "nellie.rejectDiff",
        "mac": "shift+cmd+backspace",
        "key": "shift+ctrl+backspace"
      },
      {
        "command": "nellie.rejectDiff",
        "mac": "cmd+z",
        "key": "ctrl+z",
        "when": "nellie.diffVisible"
      },
      {
        "command": "nellie.quickEditHistoryUp",
        "mac": "up",
        "key": "up",
        "when": "false && nellie.quickEditHistoryFocused"
      },
      {
        "command": "nellie.quickEditHistoryDown",
        "mac": "down",
        "key": "down",
        "when": "false && nellie.quickEditHistoryFocused"
      },
      {
        "command": "nellie.acceptVerticalDiffBlock",
        "mac": "alt+cmd+y",
        "key": "alt+ctrl+y"
      },
      {
        "command": "nellie.rejectVerticalDiffBlock",
        "mac": "alt+cmd+n",
        "key": "alt+ctrl+n"
      },
      {
        "command": "nellie.quickEdit",
        "mac": "cmd+i",
        "key": "ctrl+i"
      },
      {
        "command": "nellie.debugTerminal",
        "mac": "cmd+shift+r",
        "key": "ctrl+shift+r"
      },
      {
        "command": "nellie.toggleFullScreen",
        "mac": "cmd+k cmd+m",
        "key": "ctrl+k ctrl+m",
        "when": "!terminalFocus"
      },
      {
        "command": "nellie.toggleTabAutocompleteEnabled",
        "mac": "cmd+k cmd+a",
        "key": "ctrl+k ctrl+a",
        "when": "!terminalFocus"
      },
      {
        "command": "nellie.loadRecentChat",
        "mac": "cmd+0",
        "key": "ctrl+0"
      },
      {
        "command": "nellie.resizeAuxiliaryBarWidth",
        "mac": "cmd+\\",
        "key": "ctrl+\\"
      },
      {
        "command": "nellie.viewHistory",
        "mac": "cmd+h",
        "key": "ctrl+h"
      }
    ],
    "submenus": [
      {
        "id": "nellie.continueSubMenu",
        "label": "Nellie IDE"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "nellie.quickEdit"
        },
        {
          "command": "nellie.toggleCreator"
        },
        {
          "command": "nellie.toggleSearch"
        },
        {
          "command": "nellie.toggleInventorySettings"
        },
        {
          "command": "nellie.focusContinueInput"
        },
        {
          "command": "nellie.focusContinueInputWithoutClear"
        },
        {
          "command": "nellie.debugTerminal"
        },
        {
          "command": "nellie.toggleFullScreen"
        },
        {
          "command": "nellie.newSession"
        },
        {
          "command": "nellie.showInteractiveContinueTutorial"
        }
      ],
      "editor/context": [
        {
          "submenu": "nellie.continueSubMenu",
          "group": "0_acontinue"
        }
      ],
      "editor/title/run": [
        {
          "command": "nellie.rejectDiff",
          "group": "Nellie IDE",
          "when": "nellie.streamingDiff"
        }
      ],
      "nellie.continueSubMenu": [
        {
          "command": "nellie.focusContinueInputWithoutClear",
          "group": "Nellie IDE",
          "when": "editorHasSelection"
        },
        {
          "command": "nellie.writeCommentsForCode",
          "group": "Nellie IDE",
          "when": "editorHasSelection"
        },
        {
          "command": "nellie.writeDocstringForCode",
          "group": "Nellie IDE",
          "when": "editorHasSelection"
        },
        {
          "command": "nellie.fixCode",
          "group": "Nellie IDE",
          "when": "editorHasSelection"
        },
        {
          "command": "nellie.optimizeCode",
          "group": "Nellie IDE",
          "when": "editorHasSelection"
        },
        {
          "command": "nellie.fixGrammar",
          "group": "Nellie IDE",
          "when": "editorHasSelection && editorLangId == 'markdown'"
        }
      ],
      "explorer/context": [
        {
          "command": "nellie.selectFilesAsContext",
          "group": "1_debug@1"
        }
      ],
      "view/title": [
        {
          "command": "nellie.newSession",
          "group": "navigation@1",
          "when": "view == nellie.chatView"
        },
        {
          "command": "nellie.newSessionSearch",
          "group": "navigation@1",
          "when": "view == nellie.searchView"
        },
        {
          "command": "nellie.toggleFullScreen",
          "group": "navigation@1",
          "when": "view == nellie.chatView"
        },
        {
          "command": "nellie.viewHistory",
          "group": "navigation@1",
          "when": "view == nellie.chatView"
        },
        {
          "command": "nellie.viewHistorySearch",
          "group": "navigation@1",
          "when": "view == nellie.searchView"
        }
      ],
      "editor/title": [
        {
          "command": "nellie.toggleFullScreen",
          "group": "navigation@1",
          "when": "activeWebviewPanelId == nellie.chatView || activeWebviewPanelId == nellie.searchView"
        }
      ],
      "terminal/context": [
        {
          "command": "nellie.debugTerminal",
          "group": "navigation@top"
        }
      ]
    },
    "viewsContainers": {
      "auxiliarybar": [
        {
          "id": "nellieChat",
          "title": "Nellie IDE Chat",
          "icon": "media/sidebar-icon.png"
        },
        {
          "id": "nellieSearch",
          "title": "Nellie IDE Search",
          "icon": "media/sidebar-icon.png"
        },
        {
          "id": "nellieMemory",
          "title": "Nellie IDE Memory",
          "icon": "media/sidebar-icon.png"
        }
      ]
    },
    "views": {
      "nellieChat": [
        {
          "type": "webview",
          "id": "nellie.chatView",
          "name": "Nellie IDE Chat",
          "visibility": "visible"
        }
      ],
      "nellieSearch": [
        {
          "type": "webview",
          "id": "nellie.searchView",
          "name": "Nellie IDE Search",
          "visibility": "visible"
        }
      ],
      "nellieMemory": [
        {
          "type": "webview",
          "id": "nellie.mem0View",
          "name": "Nellie IDE Memory",
          "visibility": "visible"
        }
      ]
    },
    "jsonValidation": [
      {
        "fileMatch": "config.json",
        "url": "./config_schema.json"
      },
      {
        "fileMatch": ".nellierc.json",
        "url": "./continue_rc_schema.json"
      }
    ]
  },
  "scripts": {
    "esbuild-base": "node scripts/esbuild.js",
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "tsc": "tsc -p ./",
    "tsc-watch": "tsc -watch -p ./",
    "rebuild": "electron-rebuild -v 19.1.8 node-pty",
    "lint": "eslint src --ext ts",
    "build-test": "npm run esbuild && node esbuild.test.mjs",
    "test": "npm run build-test && node ./out/runTestOnVSCodeHost.js",
    "quick-test": "npm run build-test && node ./out/runTestOnVSCodeHost.js",
    "prepackage": "node scripts/prepackage.js",
    "package": "node scripts/package.js",
    "package-all": "node scripts/package-all.js",
    "package:pre-release": "node scripts/package.js --pre-release",
    "build:rust": "cargo-cp-artifact -ac sync sync.node -- cargo build --manifest-path ../../sync/Cargo.toml --message-format=json-render-diagnostics",
    "build-debug:rust": "npm run build:rust --",
    "build-release:rust": "npm run build:rust -- --release"
  },
